ype: String,\n      default: 'create'\n    },\n    dataPosition: {\n      type: Object,\n      default: () => {\n        return {}\n      }\n    }\n  },\n  data () {\n    return {\n      status: '',\n      position: {\n        name: '',\n        status: ''\n      },\n      items: [\n        {\n          'id': '',\n          'name': '-- Su lua chon ---'\n        },\n        {\n          'id': 1,\n          'name': 'Hiển thị'\n        },\n        {\n          'id': 0,\n          'name': 'Không hiển thị'\n        }\n      ]\n    }\n  },\n  methods: {\n    ...mapActions(['fetchApi']),\n    setInitData () {\n      let dataPosition = { ...this.dataPosition }\n      this.position = { ...this.position, ...dataPosition }\n    },\n    validateBeforeSubmit () {\n      this.$validator.validateAll().then(result => {\n        if (result) {\n          let position = Object.assign({}, this.position)\n          position.status = position.status ? 1 : 0\n          let submitData = { ...position }\n          this.$emit('submit', submitData)\n        } else {\n          this.$store.dispatch('showNotify', {\n            text: this.$t('alert.invalid'),\n            color: 'warning'\n          })\n        }\n      })\n    }\n  },\n  created () {\n    !!this.dataPosition && this.setInitData()\n  }\n}\n</script>\n"],"sourceRoot":"src/views/position"}]}